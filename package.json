{
  "name": "hypersave",
  "version": "1.0.2",
  "description": "Official TypeScript/JavaScript SDK for the Hypersave API - Your AI-powered memory layer",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "echo \"No tests yet\" && exit 0",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "hypersave",
    "ai",
    "memory",
    "knowledge-graph",
    "rag",
    "search",
    "embeddings",
    "vector-database",
    "personal-ai",
    "second-brain",
    "api-client",
    "sdk"
  ],
  "author": "Hypersave",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hypersave/hypersave-sdk"
  },
  "homepage": "https://docs.hypersave.io",
  "bugs": {
    "url": "https://github.com/hypersave/hypersave-sdk/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "peerDependencies": {},
  "dependencies": {}
}
